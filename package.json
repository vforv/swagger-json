{
  "name": "swagger-json",
  "version": "2.0.0",
  "description": "Create json doc for swagger",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "coverage",
      "test"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "dependencies": {
    "joi-to-swagger": "^6.0.1"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "express": "^4.17.1",
    "istanbul": "^0.4.5",
    "joi": "^17.4.2",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "supertest": "^6.1.6"
  },
  "peerDependencies": {
    "joi": "17.x.x"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc --check-coverage --lines 95 --functions 100 --branches 100 --statements 95 mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vforv/swagger-json.git"
  },
  "keywords": [
    "middleware",
